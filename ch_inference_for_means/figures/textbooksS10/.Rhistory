linResPlot <- function(x, y, axes=FALSE, wBox=TRUE, wLine=TRUE, lCol='#00000088', lty=1, lwd=1, xlab='', ylab='',marRes=NULL,...){#
	plot(x,y,axes=axes,xlab='',ylab=ylab,...)#
	if(wBox){#
		box()#
	}#
	g <- lm(y ~ x)#
	if(wLine){#
		abline(g, col=lCol, lty=lty, lwd=lwd)#
	}#
	if(!is.null(marRes)[1]){#
		par(mar=marRes)#
	}#
	plot(x,g$residuals, axes=axes, xlab=xlab, ylab='', ...)#
	if(wBox){#
		box()#
	}#
	abline(h=0, lty=3, col=lCol)#
}
x <- 1:50
y <- x + rnorm(x, sd=15)
linResPlot(x,y)
par(mfrow=2:1)
linResPlot(x,y)
linResPlot <- function(x, y, axes=FALSE, wBox=TRUE, wLine=TRUE, lCol='#00000088', lty=1, lwd=1, xlab='', ylab='', marRes=NULL, col='#22558866', pch=20, cex=1.5, ...){#
	plot(x, y, axes=axes, xlab='', ylab=ylab, col=col,#
