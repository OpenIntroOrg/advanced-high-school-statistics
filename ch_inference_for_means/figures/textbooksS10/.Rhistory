linResPlot <- function(x, y, axes=FALSE, wBox=TRUE, wLine=TRUE, lCol='#00000088', lty=1, lwd=1, xlab='', ylab='', marRes=NULL, col='#22558866', pch=20, cex=1.5, yR=0.1, ylim=NULL, ...){#
	if(is.null(ylim)[1]){#
		YR <- range(y) + c(-1,1)*yR*diff(range(y))#
	} else {#
		YR <- ylim#
	}#
	plot(x, y, axes=axes, xlab='', ylab=ylab, col=col,#
		pch=pch, cex=cex, ylim=YR, ...)#
	if(wBox){#
		box()#
	}#
	g <- lm(y ~ x)#
	if(wLine){#
		abline(g, col=lCol, lty=lty, lwd=lwd)#
	}#
	if(!is.null(marRes)[1]){#
		par(mar=marRes)#
	}#
	y <- g$residuals#
	YR <- range(y) + c(-1,1)*yR*diff(range(y))#
	plot(x,g$residuals, axes=axes, xlab=xlab, ylab='', col=col,#
		pch=pch, cex=cex, ylim=YR, ...)#
	if(wBox){#
		box()#
	}#
	abline(h=0, lty=3, col=lCol)#
}
